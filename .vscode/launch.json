  "version": "0.2.0",
  "configurations": [
    {
      "name": "cp_5",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "python": "${workspaceRoot}/.venv/bin/python",
      "module": "uvicorn",
      "args": [
        "--reload",
        "--host",
        "127.0.0.1",
        "--port",
        "8000",
        "--log-level",
        "debug",
        "main:api"
      ],
      "cwd": "${workspaceFolder}/cp_5",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ],
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "set_environment",
      "postDebugTask": "killdebugger",
      "jinja": true
    },
